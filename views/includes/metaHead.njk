<link rel="stylesheet" type="text/css" href="{{ 'stylesheets/style.css' | getCachebustedPath }}">
<link rel="stylesheet" href="https://use.typekit.net/yyc1dgh.css">

<script id="script-config">
(function() {
    window.AppConfig = {
        environment: '{{ appData.environment }}',
        isTestServer: {% if appData.isTestServer %}true{% else %}false{% endif %},
        locale: '{{ locale }}',
        localePrefix: '{{ localePrefix }}',
        sessionExpirySeconds: {{ appData.config.get('session.expiryInSeconds') }},
        apply: {
            enableSessionExpiryWarning: {{ appData.config.get('awardsForAll.enableSessionExpiryWarning') }}
        }
    };

    var docEl = document.documentElement;
    docEl.className = docEl.className.replace('no-js', 'js-on');

    if (
        navigator.userAgent.indexOf('MSIE') !== -1 ||
        navigator.appVersion.indexOf('Trident/') > 0
    ) {
        docEl.className += ' ' + 'is-ie';
    }

    if (sessionStorage.fontsLoaded) {
        docEl.className += ' ' + 'fonts-loaded';
    }

})();
</script>
<script src="{{ 'javascripts/head.js' | getCachebustedPath }}" id="script-head"></script>
