{% from "components/buttons.njk" import startButton %}
{% from "components/print-button/macro.njk" import printButton %}

{% extends "layouts/main.njk" %}
{% set bodyClass = 'has-static-header' %}

{% block content %}
    <main role="main" id="content">
        <div class="content-box u-inner-wide-only accent--{{ pageAccent }} a--border-top">
            <h1>{{ title }}</h1>

            <div class="s-prose accent-content--{{ pageAccent }} u-constrained-wide">
                <div class="message">
                    The Youth Capacity Fund closed for applications on <strong>Monday 19 November at 12 noon</strong>. Please go to the <a href="/funding/programmes">funding section of our website</a> to see our open funding programmes.
                </div>
            </div>
        </div>
    </main>
{% endblock %}
