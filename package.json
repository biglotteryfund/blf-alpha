{
  "name": "blf-alpha",
  "description": "Big Lottery Fund Alpha",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startDev": "export DEBUG=blf-alpha:*,express-session,connect:session-sequelize; nodemon --inspect ./bin/www",
    "test": "eslint . && jest --maxWorkers=4",
    "test-cypress": "wait-on http://localhost:8090 && cypress run",
    "pretest-ci": "npm run build",
    "test-ci": "./bin/start-test-server & concurrently --success --raw 'npm test' 'npm run test-cypress'",
    "build": "gulp clean && gulp css && webpack",
    "build-production": "export NODE_ENV=production && npm run build && gulp manifest",
    "build-autotrack": "npx autotrack -o public/autotrack/autotrack.js -p outboundLinkTracker,eventTracker,maxScrollTracker",
    "prewatch": "npm run build",
    "watch": "concurrently --kill-others 'webpack --watch' 'gulp watch'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.scss": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write",
      "eslint"
    ],
    "{controllers/routes.js,controllers/aliases.js,config/default.json}": [
      "./bin/generate-cloudfront-config"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "babel": {
    "plugins": [
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              ">0.25% in GB",
              "ie >= 10",
              "not op_mini all"
            ]
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "browserslist": [
    ">0.25% in GB",
    "ie >= 10",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "assets",
      "public"
    ],
    "ext": "js,json,yml"
  },
  "dependencies": {
    "absolution": "^1.0.0",
    "aws-sdk": "^2.259.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "config": "^1.26.2",
    "connect-session-sequelize": "^5.2.0",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "^3.1.0",
    "dompurify": "^1.0.3",
    "express": "^4.16.3",
    "express-ab": "^0.7.1",
    "express-cache-controller": "^1.0.1",
    "express-session": "^1.15.5",
    "express-validator": "^5.0.0",
    "fitvids": "^2.0.0",
    "fontfaceobserver": "^2.0.13",
    "globby": "^8.0.1",
    "helmet": "^3.8.1",
    "html-to-text": "^3.3.0",
    "i18n-2": "^0.7.2",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "jsdom": "^11.5.1",
    "jsonwebtoken": "^8.3.0",
    "juice": "^4.2.2",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "ms": "^2.1.1",
    "mysql2": "^1.4.2",
    "nodemailer": "^4.1.0",
    "nunjucks": "^3.1.3",
    "object-hash": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "query-string": "^5.0.1",
    "raven": "2.6.3",
    "raven-js": "3.26.4",
    "req-flash": "0.0.3",
    "request": "^2.84.0",
    "request-promise-native": "^1.0.5",
    "response-time": "^2.3.2",
    "sequelize": "^4.27.0",
    "serve-favicon": "^2.5.0",
    "sitemap": "^1.13.0",
    "slugify": "^1.2.1",
    "snyk": "^1.83.0",
    "sqlite3": "^4.0.0",
    "uuid": "^3.2.1",
    "vary": "^1.1.1",
    "vue": "^2.5.16",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "autoprefixer": "9.0.2",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "common-tags": "^1.8.0",
    "concurrently": "3.6.1",
    "cssnano": "4.0.5",
    "cypress": "3.0.2",
    "del": "^3.0.0",
    "dotenv": "6.0.0",
    "eslint": "^4.19.0",
    "eslint-plugin-vue": "^4.5.0",
    "gulp": "3.9.1",
    "gulp-cli": "2.0.1",
    "gulp-postcss": "7.0.1",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.4",
    "husky": "^0.14.3",
    "jest": "23.4.1",
    "lint-staged": "7.2.0",
    "livereload": "^0.7.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "1.18.3",
    "postcss-fout-with-a-class": "1.1.0",
    "prettier": "1.14.0",
    "prompt": "^1.0.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "wait-on": "^2.1.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^2.1.5"
  },
  "snyk": true
}
