{
  "name": "biglotteryfund",
  "description": "Big Lottery Fund",
  "version": "1.0.0",
  "private": true,
  "config": {
    "dist": {
      "root": "public/build",
      "css": "public/build/latest/stylesheets",
      "js": "public/build/latest/javascripts"
    }
  },
  "scripts": {
    "start": "node ./bin/www",
    "startDev": "export DEBUG=biglotteryfund:*,express-session,connect:session-sequelize; nodemon --inspect ./bin/www",
    "test": "npm audit && eslint . && jest --env=node --maxWorkers=4",
    "test-cypress": "wait-on http://localhost:8090 && cypress run",
    "pretest-ci": "npm run build",
    "test-ci": "./bin/start-test-server & concurrently --success --raw 'npm test' 'npm run test-cypress'",
    "clean": "del-cli $npm_package_config_dist_root/* ./config/assets.json",
    "build-sass": "node-sass assets/sass --output $npm_package_config_dist_css --source-map $npm_package_config_dist_css",
    "build-css": "npm run build-sass && postcss $npm_package_config_dist_css/*.css --replace --map",
    "build-js": "webpack",
    "build": "npm run clean && npm run build-css && npm run build-js",
    "build-production": "export NODE_ENV=production && npm run build && ./bin/hash-rev",
    "build-autotrack": "npx autotrack -o public/autotrack/autotrack.js -p outboundLinkTracker,eventTracker,maxScrollTracker",
    "watch-livereload": "livereload $npm_package_config_dist_css/style.css",
    "watch-css": "watch 'npm run build-css' assets/sass",
    "watch-js": "webpack --watch",
    "watch": "npm run build && concurrently --kill-others 'npm run watch-js' 'npm run watch-css' 'npm run watch-livereload'",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.scss": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write",
      "eslint"
    ],
    "{controllers/routes.js,controllers/aliases.js,config/default.json}": [
      "./bin/generate-cloudfront-config"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "babel": {
    "plugins": [
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              ">0.25% in GB",
              "ie >= 10",
              "not op_mini all"
            ]
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "browserslist": [
    ">0.25% in GB",
    "ie >= 10",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "assets",
      "public"
    ],
    "ext": "js,json,yml"
  },
  "dependencies": {
    "aws-sdk": "^2.259.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "config": "^1.26.2",
    "connect-session-sequelize": "^5.2.0",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "^3.1.0",
    "dompurify": "^1.0.3",
    "express": "^4.16.3",
    "express-ab": "^0.7.1",
    "express-cache-controller": "^1.0.1",
    "express-session": "^1.15.5",
    "express-validator": "^5.0.0",
    "fitvids": "^2.0.0",
    "fontfaceobserver": "^2.0.13",
    "globby": "^8.0.1",
    "helmet": "^3.8.1",
    "html-to-text": "^3.3.0",
    "i18n-2": "^0.7.2",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "jsdom": "^11.5.1",
    "jsonwebtoken": "^8.3.0",
    "juice": "^4.2.2",
    "lodash": "^4.17.10",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "ms": "^2.1.1",
    "mysql2": "^1.4.2",
    "nodemailer": "^4.1.0",
    "nunjucks": "^3.1.3",
    "object-hash": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "query-string": "^5.0.1",
    "raven": "2.6.3",
    "raven-js": "3.26.4",
    "request": "^2.84.0",
    "request-promise-native": "^1.0.5",
    "response-time": "^2.3.2",
    "sequelize": "4.38.0",
    "serve-favicon": "^2.5.0",
    "sitemap": "^1.13.0",
    "slugify": "^1.2.1",
    "sqlite3": "4.0.2",
    "uuid": "^3.2.1",
    "vary": "^1.1.1",
    "vue": "2.5.17",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "autoprefixer": "9.1.1",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "concurrently": "3.6.1",
    "css-loader": "1.0.0",
    "cssnano": "4.0.5",
    "cypress": "3.1.0",
    "del-cli": "1.1.0",
    "dotenv": "6.0.0",
    "eslint": "^4.19.0",
    "eslint-plugin-vue": "4.7.1",
    "husky": "^0.14.3",
    "jest": "23.4.1",
    "lint-staged": "7.2.0",
    "livereload": "^0.7.0",
    "node-mocks-http": "^1.7.0",
    "node-sass": "4.9.3",
    "nodemon": "1.18.3",
    "postcss-cli": "6.0.0",
    "postcss-fout-with-a-class": "1.1.0",
    "prettier": "1.14.0",
    "prompt": "^1.0.0",
    "vue-loader": "15.2.6",
    "vue-template-compiler": "2.5.17",
    "wait-on": "^2.1.0",
    "watch": "1.0.2",
    "webpack": "4.16.3",
    "webpack-cli": "3.1.0"
  }
}
