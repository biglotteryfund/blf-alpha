{
  "name": "blf-alpha",
  "description": "Big Lottery Fund Alpha",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startDev": "export DEBUG=blf-alpha:*; nodemon ./bin/www",
    "lint": "eslint . bin/* --ignore-pattern public/ --ignore-pattern views/",
    "test-deps": "snyk test",
    "test-client": "mocha assets/**/*.test.js --bail --exit",
    "test-server": "nyc --include={middleware,modules,services}/**/*.js --all mocha {controllers,middleware,modules,services}/{**/*,*}.test.js --bail --exit",
    "test-unit": "npm run test-client && npm run test-server",
    "test-integration": "mocha integration-tests/integration-tests.js --bail --exit --timeout 20s",
    "test": "npm run test-unit && npm run test-integration",
    "build": "gulp build && webpack --mode=development",
    "build-production": "export NODE_ENV=production && gulp build && webpack --mode=production && gulp manifest",
    "build-autotrack": "autotrack -o public/autotrack/autotrack.js -p outboundLinkTracker,eventTracker,maxScrollTracker",
    "prewatch": "npm run build",
    "watch": "concurrently --kill-others 'webpack --mode=development --watch' 'gulp watch'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "assets/**/*.scss": [
      "prettier --write"
    ],
    "{assets,controllers,integration-tests,lambda,middleware,models,modules,services}/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "{gulpfile.js,server.js}": [
      "prettier --write",
      "eslint"
    ],
    "{controllers/routes.js,controllers/aliases.js,config/default.json}": [
      "./bin/generate-cloudfront-config"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-object-assign",
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 10"
            ]
          }
        }
      ]
    ]
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "assets",
      "public"
    ],
    "ext": "js,json,yml",
    "exec": "bin/www"
  },
  "dependencies": {
    "absolution": "^1.0.0",
    "aws-sdk": "^2.110.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "config": "^1.26.2",
    "connect-session-sequelize": "^5.2.0",
    "connect-slashes": "^1.3.1",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "^3.1.0",
    "express": "^4.15.5",
    "express-ab": "^0.7.1",
    "express-cache-controller": "^1.0.1",
    "express-session": "^1.15.5",
    "express-validator": "^5.0.0",
    "fitvids": "^2.0.0",
    "fontfaceobserver": "^2.0.13",
    "hammerjs": "^2.0.8",
    "helmet": "^3.8.1",
    "html-to-text": "^3.3.0",
    "i18n-2": "^0.7.2",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "jsdom": "^11.3.0",
    "jsonwebtoken": "^8.1.0",
    "juice": "^4.2.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "ms": "^2.1.1",
    "mysql2": "^1.4.2",
    "nodemailer": "^4.1.0",
    "nunjucks": "^3.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "query-string": "^5.0.1",
    "raven": "^2.3.0",
    "raven-js": "^3.18.1",
    "req-flash": "0.0.3",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "^4.8.0",
    "serve-favicon": "^2.4.3",
    "shortid": "^2.2.8",
    "slugify": "^1.2.1",
    "snyk": "^1.69.6",
    "sqlite3": "^3.1.13",
    "swiper": "^4.1.6",
    "uuid": "^3.1.0",
    "vary": "^1.1.1",
    "vue": "^2.4.4",
    "sitemap": "^1.13.0",
    "xss": "^0.3.4",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "autotrack": "^2.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.0",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "dotenv": "^5.0.0",
    "eslint": "^4.8.0",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "he": "^1.1.1",
    "husky": "^0.14.3",
    "import-fresh": "^2.0.0",
    "lint-staged": "^7.0.0",
    "livereload": "^0.7.0",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "node-mocks-http": "^1.6.7",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.4.1",
    "p-settle": "^2.0.0",
    "postcss-fout-with-a-class": "^1.1.0",
    "prettier": "^1.8.2",
    "pretty": "^2.0.0",
    "prompt": "^1.0.0",
    "run-sequence": "^2.2.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "snyk": true
}
