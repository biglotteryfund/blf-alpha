{
  "name": "blf-alpha",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "export DEBUG=blf-alpha:*; nodemon ./bin/www",
    "lint": "eslint .",
    "test": "npm run lint && gulp test",
    "test-deps": "snyk test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "precommit": "lint-staged",
    "prepush": "npm test"
