      "prettier --write",
      "eslint",
      "git add"
    ],
    "assets/**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true
  },
