{
  "name": "blf-alpha",
  "description": "Big Lottery Fund Alpha",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startDev": "export DEBUG=blf-alpha:*; nodemon ./bin/www",
    "lint": "eslint .",
    "test-unit": "mocha {assets,modules}/**/*.test.js --exit",
    "test-integration": "mocha integration-tests/*.test.js --exit --timeout 20s",
    "test": "npm run test-unit && npm run test-integration",
    "test-deps": "snyk test",
    "build": "gulp build",
    "build-production": "export NODE_ENV=production && gulp build:production",
    "build-autotrack": "autotrack -o public/autotrack/autotrack.js -p maxScrollTracker",
    "watch-js": "webpack --watch",
    "watch-assets": "gulp watch",
    "prewatch": "npm run build",
    "watch": "concurrently --kill-others 'npm run watch-js' 'npm run watch-assets'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "{assets,bin,controllers,models,modules,test}/**/*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "{gulpfile.js,server.js}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "assets/**/*.scss": [
      "prettier --write",
      "git add"
    ],
    "controllers/routes.js": [
      "./bin/scripts/generate-cloudfront-config.js"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-object-assign",
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 10"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "absolution": "^1.0.0",
    "aws-sdk": "^2.110.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "config": "^1.26.2",
    "connect-session-sequelize": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "csurf": "^1.9.0",
    "debug": "~2.6.0",
    "eslint": "^4.8.0",
    "express": "^4.15.5",
    "express-ab": "^0.7.1",
    "express-cache-controller": "^1.0.1",
    "express-session": "^1.15.5",
    "express-validator": "^4.2.1",
    "generate-schema": "^2.5.1",
    "helmet": "^3.8.1",
    "i18n-2": "^0.6.3",
    "jquery": "^3.2.1",
    "jsdom": "^11.3.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "mysql2": "^1.4.2",
    "nodemailer": "^4.1.0",
    "nunjucks": "^3.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "query-string": "^5.0.1",
    "raven": "^2.2.1",
    "raven-js": "^3.18.1",
    "req-flash": "0.0.3",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rootbeer": "^1.0.5",
    "sequelize": "^4.8.0",
    "serve-favicon": "^2.4.3",
    "shortid": "^2.2.8",
    "slugify": "^1.2.1",
    "snyk": "^1.41.1",
    "sqlite3": "^3.1.13",
    "swiper": "^3.4.2",
    "uuid": "^3.1.0",
    "vary": "^1.1.1",
    "vue": "^2.4.4",
    "winston": "^2.3.1",
    "xss": "^0.3.4",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "autotrack": "^2.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^3.5.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-cli": "^1.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "husky": "^0.14.3",
    "import-fresh": "^2.0.0",
    "import-lazy": "^3.0.0",
    "lint-staged": "^4.2.3",
    "livereload": "^0.6.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.2",
    "prompt": "^1.0.0",
    "puppeteer": "^0.11.0",
    "run-sequence": "^2.2.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.7.1"
  },
  "snyk": true
}
